{
  "name": "webpack-react-startup",
  "version": "1.0.5",
  "description": "webpack react startup without create-react-app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "proxy": "webpack-dev-server --mode development --open --hot --env proxy",
